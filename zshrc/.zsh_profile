export GOPATH=$HOME/go
export PATH=$PATH:$HOME/.yarn/bin:~/.local/bin/:/usr/local/bin:$HOME/tool/:$GOPATH/bin
powerline-daemon -q
if [ -e $HOME/.local/lib/python3.8 ]; then
  . ~/.local/lib/python3.8/site-packages/powerline/bindings/zsh/powerline.zsh
elif [ -e $HOME/.local/lib/python3.6 ]; then
  . ~/.local/lib/python3.6/site-packages/powerline/bindings/zsh/powerline.zsh
fi
if [ -e $HOME/.pyenv ]; then
  export PYENV_ROOT="$HOME/.pyenv"
  export PATH="$PYENV_ROOT/bin:$PATH"
  eval "$(pyenv init -)"
  eval "$(pyenv virtualenv-init -)"
fi
if [ -e $HOME/.rbenv ]; then
  export PATH="$HOME/.rbenv/bin:$PATH"
  eval "$(rbenv init -)"
fi
